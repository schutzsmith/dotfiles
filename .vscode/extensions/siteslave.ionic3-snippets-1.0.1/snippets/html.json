  "ion-card-image": {
    "prefix": "ion3-card-image",
    "body": [
    "<ion-card>",
    "\t<img src=\"$1\"/>",
    "\t<ion-card-content>",
    "\t\t<ion-card-title>",
    "\t\t\t$2",
    "\t\t</ion-card-title>",
