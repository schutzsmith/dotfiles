  "ion-card-social": {
    "prefix": "ion3-card-social",
    "body": [
      "<ion-card>",
      "\t<ion-item>",
      "\t\t<ion-avatar item-left>",
      "\t\t\t<img src=\"$1\">",
      "\t\t</ion-avatar>",
      "\t\t<h2>$2</h2>",
      "\t\t<p>$3</p>",
      "\t</ion-item>",
      "\t<img src=\"$4\">",
      "\t<ion-card-content>",
      "\t\t<p>$5</p>",
      "\t</ion-card-content>",
      "\t<ion-row>",
      "\t\t<ion-col>",
      "\t\t\t<button ion-button icon-left clear small>",
      "\t\t\t\t<ion-icon name=\"thumbs-up\"></ion-icon>",
      "\t\t\t\t<div>$6</div>",
      "\t\t\t</button>",
      "\t\t</ion-col>",
      "\t\t<ion-col>",
      "\t\t\t<button ion-button icon-left clear small>",
      "\t\t\t\t<ion-icon name=\"text\"></ion-icon>",
      "\t\t\t\t<div>$7</div>",
      "\t\t\t</button>",
      "\t\t</ion-col>",
      "\t\t<ion-col center text-center>",
      "\t\t\t<ion-note>",
      "\t\t\t\t$8",
      "\t\t\t</ion-note>",
      "\t\t</ion-col>",
      "\t</ion-row>",
      "</ion-card>"
    ]
  },
  "ion-card-map": {
    "prefix": "ion3-card-map",
    "body": [
      "<ion-card>",
      "\t<img src=\"$1\">",
      "\t<ion-fab right top>",
      "\t\t<button ion-fab $2>",
      "\t\t\t<ion-icon name=\"pin\"></ion-icon>",
      "\t\t</button>",
      "\t</ion-fab>",
      "\t<ion-item>",
      "\t\t<ion-icon name=\"$3\" item-left large $4></ion-icon>",
      "\t\t<h2>$5</h2>",
      "\t\t<p>$6</p>",
      "\t</ion-item>",
      "\t<ion-item>",
      "\t\t<span item-left>${7:time-left}</span>",
      "\t\t<span item-left>${8:distance}</span>",
      "\t\t<button ion-button icon-left clear item-right $9>",
      "\t\t\t<ion-icon name=\"navigate\"></ion-icon>",
      "\t\t\t\t${10:start-navigate-text}",
      "\t\t</button>",
      "\t</ion-item>",
      "</ion-card>"
    ]
  },
